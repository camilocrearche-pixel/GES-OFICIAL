+:root {
+  --bg: #f7f4ff;
+  --card: #ffffff;
+  --text: #1c1930;
+  --muted: #6f6b85;
+  --primary: #5f38f5;
+  --secondary: #ed60a9;
+  --accent: #14c8b0;
+  --shadow: 0 16px 40px rgba(33, 20, 95, 0.14);
+}
+
+* {
+  box-sizing: border-box;
+  margin: 0;
+  padding: 0;
+}
+
+body {
+  font-family: "Montserrat", sans-serif;
+  color: var(--text);
+  background: radial-gradient(circle at 100% 0%, #efe8ff 0%, #f9f7ff 30%, #ffffff 100%);
+}
+
+a {
+  text-decoration: none;
+  color: inherit;
+}
+
+.topbar {
+  background: var(--text);
+  color: white;
+  text-align: center;
+  padding: 0.65rem;
+  font-size: 0.85rem;
+}
+
+.header {
+  max-width: 1140px;
+  margin: 0 auto;
+  padding: 1.15rem 1rem;
+  display: flex;
+  align-items: center;
+  justify-content: space-between;
+  gap: 1rem;
+}
+
+.logo {
+  font-weight: 800;
+  font-size: 1.4rem;
+  letter-spacing: -0.5px;
+}
+
+.logo span {
+  color: var(--primary);
+}
+
+.header nav {
+  display: flex;
+  gap: 1.2rem;
+  font-weight: 600;
+  color: #3d3960;
+  font-size: 0.93rem;
+}
+
+.cta-header {
+  border: none;
+  background: linear-gradient(95deg, var(--primary), #7b52ff);
+  color: white;
+  padding: 0.65rem 1rem;
+  border-radius: 999px;
+  font-weight: 700;
+  cursor: pointer;
+}
+
+main {
+  max-width: 1140px;
+  margin: 0 auto;
+  padding: 0 1rem 3rem;
+}
+
+.hero {
+  margin-top: 0.8rem;
+  padding: 3rem;
+  border-radius: 24px;
+  background: linear-gradient(135deg, #2a1f68 0%, #5f38f5 55%, #ed60a9 100%);
+  color: white;
+  display: grid;
+  gap: 1.2rem;
+  grid-template-columns: 1fr 290px;
+  align-items: center;
+  box-shadow: var(--shadow);
+}
+
+.eyebrow {
+  font-weight: 700;
+  font-size: 0.82rem;
+  opacity: 0.9;
+}
+
+.hero h1 {
+  font-size: clamp(2rem, 4vw, 3rem);
+  line-height: 1.1;
+  margin: 0.8rem 0;
+}
+
+.hero h1 span {
+  color: #b5fff5;
+}
+
+.hero p {
+  max-width: 55ch;
+  opacity: 0.95;
+}
+
+.hero-actions {
+  margin-top: 1.4rem;
+  display: flex;
+  gap: 0.8rem;
+}
+
+.btn {
+  padding: 0.8rem 1.2rem;
+  border-radius: 999px;
+  font-weight: 700;
+  font-size: 0.95rem;
+}
+
+.btn-primary {
+  background: white;
+  color: #312181;
+}
+
+.btn-secondary {
+  border: 1.5px solid rgba(255, 255, 255, 0.8);
+}
+
+.hero-card {
+  background: rgba(255, 255, 255, 0.16);
+  border: 1px solid rgba(255, 255, 255, 0.25);
+  border-radius: 18px;
+  padding: 1.2rem;
+  backdrop-filter: blur(5px);
+}
+
+.hero-card h3 {
+  font-size: 1.2rem;
+}
+
+.hero-card p {
+  font-size: 0.9rem;
+  margin: 0.45rem 0 0.75rem;
+}
+
+.price {
+  font-size: 1.4rem;
+  font-weight: 800;
+}
+
+.tag {
