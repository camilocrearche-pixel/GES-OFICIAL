+  margin-top: 0.65rem;
+  font-size: 0.76rem;
+  padding: 0.3rem 0.65rem;
+  border-radius: 999px;
+  background: #b5fff5;
+  color: #093f44;
+  font-weight: 700;
+}
+
+.categories,
+.benefits {
+  margin: 2rem 0;
+  display: grid;
+  gap: 1rem;
+  grid-template-columns: repeat(3, 1fr);
+}
+
+.categories article,
+.benefits article {
+  background: var(--card);
+  border-radius: 16px;
+  padding: 1.2rem;
+  box-shadow: 0 8px 24px rgba(35, 25, 99, 0.08);
+}
+
+.categories h4,
+.benefits h3 {
+  margin-bottom: 0.4rem;
+}
+
+.categories p,
+.benefits p,
+.section-title p {
+  color: var(--muted);
+}
+
+.section-title {
+  margin: 3rem 0 1.2rem;
+}
+
+.section-title p {
+  font-weight: 700;
+  letter-spacing: 1.2px;
+  font-size: 0.8rem;
+}
+
+.section-title h2 {
+  margin-top: 0.4rem;
+  font-size: clamp(1.4rem, 2.8vw, 2rem);
+}
+
+.grid {
+  display: grid;
+  grid-template-columns: repeat(4, minmax(180px, 1fr));
+  gap: 1rem;
+}
+
+.product-card {
+  background: white;
+  border-radius: 16px;
+  padding: 0.9rem;
+  box-shadow: 0 8px 24px rgba(38, 29, 105, 0.08);
+}
+
+.product-image {
+  width: 100%;
+  aspect-ratio: 3 / 4;
+  border-radius: 12px;
+  margin-bottom: 0.8rem;
+}
+
+.grad-1 {
+  background: linear-gradient(135deg, #8967ff, #e9d8ff);
+}
+
+.grad-2 {
+  background: linear-gradient(135deg, #ff7cb5, #ffd8ea);
+}
+
+.grad-3 {
+  background: linear-gradient(135deg, #57d9ca, #d8fff9);
+}
+
+.grad-4 {
+  background: linear-gradient(135deg, #4f4f7d, #cdcdff);
+}
+
+.product-card h3 {
+  font-size: 1rem;
+}
+
+.product-card p {
+  margin: 0.35rem 0;
+  color: var(--muted);
+  font-size: 0.9rem;
+}
+
+.newsletter {
+  margin-top: 3rem;
+  padding: 2rem;
+  border-radius: 20px;
+  background: linear-gradient(120deg, #21145f, #5f38f5);
+  color: white;
+  text-align: center;
+}
+
+.newsletter p {
+  margin: 0.55rem auto 1rem;
+  max-width: 60ch;
+  opacity: 0.92;
+}
+
+.newsletter form {
+  display: flex;
+  justify-content: center;
+  gap: 0.7rem;
+  flex-wrap: wrap;
+}
+
+.newsletter input {
+  min-width: 260px;
+  border-radius: 999px;
+  border: none;
+  padding: 0.75rem 1rem;
+}
+
+.newsletter button {
+  border: none;
+  border-radius: 999px;
+  padding: 0.75rem 1.1rem;
+  font-weight: 700;
+  background: #b5fff5;
+  color: #0e3f46;
+}
+
+footer {
+  text-align: center;
+  padding: 1.6rem 1rem 2rem;
+  color: #7c7797;
+  font-size: 0.9rem;
+}
+
+@media (max-width: 980px) {
+  .header nav {
+    display: none;
+  }
+
+  .hero {
+    grid-template-columns: 1fr;
+    padding: 2rem;
+  }
+
+  .grid {
+    grid-template-columns: repeat(2, minmax(170px, 1fr));
+  }
+}
+
+@media (max-width: 680px) {
+  .categories,
+  .benefits,
+  .grid {
+    grid-template-columns: 1fr;
+  }
+
+  .hero-actions {
+    flex-direction: column;
+    align-items: flex-start;
+  }
+
+  .btn {
+    width: 100%;
+    text-align: center;
+  }
+}
 
EOF
